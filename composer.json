{
  "name": "kakposoe/laravel-view-generator",
  "description": "A laravel view generator",
  "license": "MIT",
  "keywords": [ "laravel", "view", "generator" ],
  "type": "library",
  "authors": [
    {
      "name": "Kelvin Akposoe",
      "email": "kakposoe@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/support": "^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^5.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Kakposoe\\LaravelViewGenerator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kakposoe\\LaravelViewGenerator\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kakposoe\\LaravelViewGenerator\\LaravelViewGeneratorServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
